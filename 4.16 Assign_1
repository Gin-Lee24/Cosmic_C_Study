#define _CRT_SECURE_NO_WARNINGS

#include <stdio.h>

int main()
{
   int N;

   scanf("%d", &N);
   if (N % 2 == 0)
   {
      for (int i = 1; 2 * i <= N; i++)
         printf("%d ", 2 * i);
   }
   else
   {
      for (int i = 0; 2 * i + 1 <= N; i++)
         printf("%d ", 2 * i + 1);
   }
   
   return 0;
}
